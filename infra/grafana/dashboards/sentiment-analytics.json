      },
      "targets": [
        {
          "format": "table",
          "rawSql": "WITH current_sentiment AS (SELECT asset_id, sentiment_score FROM sentiment_aggregated_1h WHERE time > now() - interval '1 hour'), previous_sentiment AS (SELECT asset_id, avg(sentiment_score) as prev_score FROM sentiment_aggregated_1h WHERE time BETWEEN now() - interval '25 hours' AND now() - interval '24 hours' GROUP BY asset_id) SELECT a.symbol, a.name, cs.sentiment_score as current_score, ps.prev_score, (cs.sentiment_score - ps.prev_score) as sentiment_change FROM current_sentiment cs JOIN previous_sentiment ps ON cs.asset_id = ps.asset_id JOIN assets a ON cs.asset_id = a.id ORDER BY abs(cs.sentiment_score - ps.prev_score) DESC LIMIT 15",
          "refId": "A"
        }
      ],
      "title": "Biggest Sentiment Movers (24h)",
      "type": "table"
